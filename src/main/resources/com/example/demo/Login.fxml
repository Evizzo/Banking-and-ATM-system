<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.controllers.LoginController">
   <children>
      <TextField fx:id="usernameTextField" layoutX="110.0" layoutY="160.0" style="-fx-background-color: green; -fx-border-color: white;" />
      <TextField fx:id="pinTextField" layoutX="340.0" layoutY="160.0" style="-fx-background-color: green; -fx-border-color: white;" />
      <Text layoutX="130.0" layoutY="148.0" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-fill: green;" text="USERNAME">
         <font>
            <Font size="21.0" />
         </font></Text>
      <Text layoutX="399.0" layoutY="149.0" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-fill: green;" text="PIN">
         <font>
            <Font size="21.0" />
         </font></Text>
      <Button layoutX="260.0" layoutY="200.0" mnemonicParsing="false" onAction="#loginButton" style="-fx-background-color: green; -fx-border-color: white;" text="Login">
         <font>
            <Font size="21.0" />
         </font>
      </Button>
      <Button layoutX="512.0" layoutY="338.0" mnemonicParsing="false" onAction="#backButton" style="-fx-background-color: green; -fx-border-color: white;" text="Back">
         <font>
            <Font size="21.0" />
         </font>
      </Button>
      <Label fx:id="loginLabel" layoutX="14.0" layoutY="14.0" style="-fx-background-color: green; -fx-border-color: white;" text="Please insert Your data below">
         <font>
            <Font size="21.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
